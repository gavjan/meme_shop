html
    head
        title= title
    style
        include style.css
    body
        h1= message
        img(src="./assets/img/main.jpg")
        hr
        h1="The Most Expensive Memes"
        ul
            each meme in memes
                li
                    a(href="./views/" + meme.url)
                        img(src=meme.url)
                        p=meme.price